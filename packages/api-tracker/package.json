{
  "name": "@kjanat/bun-api-tracker",
  "version": "0.1.0",
  "type": "module",
  "description": "Automated Bun API detection and coverage tracking for bun-polyfills",
  "repository": {
    "type": "git",
    "url": "https://github.com/kjanat/bun-polyfills.git",
    "directory": "packages/api-tracker"
  },
  "homepage": "https://github.com/kjanat/bun-polyfills#readme",
  "bugs": {
    "url": "https://github.com/kjanat/bun-polyfills/issues"
  },
  "author": "Kaj Kowalski",
  "license": "GPL-3.0-only",
  "packageManager": "bun@1.3.4",
  "engines": {
    "bun": ">=1.3.4"
  },
  "bin": {
    "bun-api-tracker": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "files": [
    "data",
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "bun",
    "api",
    "tracker",
    "coverage",
    "polyfill"
  ],
  "scripts": {
    "badge": "bun run src/cli.ts badge",
    "build": "bun build:bundle && bun build:types",
    "build:bundle": "bun build ./src/index.ts ./src/cli.ts ./src/types.ts --outdir dist --target node",
    "build:types": "tsc --emitDeclarationOnly",
    "check": "bun run src/cli.ts check",
    "clean": "rm -rf dist output tests/file tests/spawn tests/shell tests/util tests/glob",
    "compare": "bun run src/cli.ts compare",
    "report": "bun run src/cli.ts report",
    "sync-tests": "bun run src/cli.ts sync-tests",
    "sync-tests:dry": "bun run src/cli.ts sync-tests --dry-run",
    "test": "bun test",
    "typecheck": "tsgo --noEmit"
  },
  "peerDependencies": {
    "@types/bun": "catalog:"
  },
  "dependencies": {
    "@kjanat/bun-polyfills": "workspace:*",
    "typescript": "catalog:"
  },
  "devDependencies": {
    "@types/bun": "catalog:",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:"
  }
}
